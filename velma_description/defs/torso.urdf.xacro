    </link>

    <link name="omnitorso_x">
      <inertial>
        <mass value="0.001"/>
        <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
      </inertial>
      <collision>
        <origin xyz="-0.081 0 0.1225" rpy="0.0 0 0" />
