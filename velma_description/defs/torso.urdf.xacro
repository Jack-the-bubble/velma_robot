    <joint name="torso_base_joint" type="revolute">
      <xacro:insert_block name="origin" />
      <limit lower="-20.0" upper="20.0" effort="1000" velocity="1"/>
      <axis xyz="0 0 1"/>
      <parent link="${parent}"/>
      <child link="omnitorso_x"/>
    </joint>

    <joint name="omni_joint_y" type="prismatic">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <limit lower="-20.0" upper="20.0" effort="1000" velocity="1"/>
      <axis xyz="0 1 0"/>
      <parent link="omnitorso_x"/>
      <child link="omnitorso_y"/>
    </joint>

    <joint name="omni_joint_x" type="prismatic">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <limit lower="-20.0" upper="20.0" effort="1000" velocity="1"/>
      <axis xyz="1 0 0"/>
      <parent link="omnitorso_y"/>
      <child link="torso_base"/>
    </joint>

    <transmission name="trans_rot">
      <type>transmission_interface/SimpleTransmission</type>
      <actuator name="$torso_base_motor">
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
      <joint name="torso_base_joint">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      </joint>
    </transmission>

    <transmission name="trans_x">
      <type>transmission_interface/SimpleTransmission</type>
      <actuator name="$omni_y_motor">
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
      <joint name="omni_joint_y">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      </joint>
    </transmission>

    <transmission name="trans_y">
      <type>transmission_interface/SimpleTransmission</type>
      <actuator name="$omni_x_motor">
        <mechanicalReduction>1</mechanicalReduction>
      </actuator>
      <joint name="omni_joint_x">
        <hardwareInterface>hardware_interface/PositionJointInterface</hardwareInterface>
      </joint>
    </transmission>

    <gazebo reference="omnitorso_x">
      <turnGravityOff>false</turnGravityOff>
      <selfCollide>true</selfCollide>
    </gazebo>

    <gazebo reference="omnitorso_y">
      <turnGravityOff>false</turnGravityOff>
