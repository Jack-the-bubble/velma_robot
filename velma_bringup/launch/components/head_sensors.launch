<?xml version="1.0"?>
<launch>

  <include file="$(find openni_launch)/launch/openni.launch">
    <arg name="camera"                value="right_kinect" />
    <arg name="device_id"             value="A00363A09609107A" />
		<arg name="rgb_frame_id" value="/kinect_right_rgb_optical_frame" />
		<arg name="depth_frame_id" value="/kinect_right_ir_optical_frame" />
  </include>

  <include file="$(find openni_launch)/launch/openni.launch">
    <arg name="camera"                value="left_kinect" />
    <arg name="device_id"             value="A00363921080033A" />
		<arg name="rgb_frame_id" value="/kinect_left_rgb_optical_frame" />
		<arg name="depth_frame_id" value="/kinect_left_ir_optical_frame" />
  </include>

  <include file="$(find prosilica_camera)/launch/generic.launch">
    <arg name="camera"       value="stereo_left" />
    <arg name="guid"          value="117935" />
<!--    <arg name="ip_address"   value="10.68.0.20" /> -->
    <arg name="trigger_mode" value="streaming" />
    <arg name="frame_id"     value="stereo_left_optical_frame" />
  </include>

  <node ns="stereo_left" pkg="image_proc" type="image_proc" respawn="false" name="prosilica_image_proc" />

</launch>
